.indexImage {
    position absolute
    width 100%;
    height 560px;
}

.introduceBox {
    position relative;
    .indexBg{
        user-select none;
        width 100%;
        height 556px;
        overflow hidden;
        background: rgba(7,35,68, .4);
        text-align center;
        .indexTitle {
            font-family: "Microsoft YaHei";
            font-size 1.1rem;
            color #F5F9FF;
            margin-top 4.38rem;
        }
        .indexTip{
